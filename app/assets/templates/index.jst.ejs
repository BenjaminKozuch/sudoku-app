<h1>Puzzles</h1>
<a class="puzzle-index" href="#puzzles/new">New Puzzle</a>

<% puzzles.each(function (puzzle) { %>
  <ul class="puzzle-index group">
    <a href="#puzzles/<%= puzzle.id %>">
      <li>Puzzle <%= puzzle.id %></li>
    </a>
    <a href="#puzzles/<%= puzzle.id %>/solution">
      <li>Solution</li>
    </a>
    <a href="#puzzles/<%= puzzle.id %>/original">
      <li>Original</li>
    </a>
  </ul>
<% }) %>
